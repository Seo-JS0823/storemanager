<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.storemanager.member.MemberMapper">
	<!-- 로그인 -->
	<select id="login" resultType="com.storemanager.member.MemberDTO">
		SELECT
			*
		FROM
			GE_MEMBER
		WHERE
			GM_ID = #{gm_id}
	</select>
	
	<!-- 회원가입 -->
	<insert id="join">
		INSERT INTO GE_MEMBER
			(GM_ID, GM_LEVEL, GM_PWD, GM_NAME, GM_EMAIL, GM_TEL, GM_PATH)
		VALUES
			(#{gm_id}, #{gm_level}, #{gm_pwd}, #{gm_name}, #{gm_email}, #{gm_tel}, #{gm_path})
	</insert>
	
	<!-- 회원가입시 존재하는 아이디인지 검증 -->
	<select id="joinIdValidation" resultType="int">
		SELECT
			count(*)
		FROM
			GE_MEMBER
		WHERE
			GM_ID = #{gm_id}
	</select>
	
	<!-- 회원정보 변경 -->
	<update id="profileUpdate">
		UPDATE GE_MEMBER SET
			GM_LEVEL = #{gm_level},
			GM_EMAIL = #{gm_email},
			GM_TEL = #{gm_tel}
			<if test="gm_pwd != 'FALSE'">
				, GM_PWD = #{gm_pwd}
			</if>
			<if test="gm_path != null">
				, GM_PATH = #{gm_path}
			</if>
		WHERE
			GM_ID = #{gm_id}
	</update>
